import sys

s = sys.stdin.readlines()  # считывание списка из входного потока
lst_in = [list(map(int, x.strip().split())) for x in s]  # создаем двумерный список и каждый его элемент приводим к типу integer

a_copy = lst_in.copy()  # создаем копию изначального списка

for i in range(len(lst_in)):  # пробегаем вложенные списки
    for j in range(i+1, len(lst_in)):  # пробегаем элементы в них; плюс единица к i необязательна, она лишь убирает конструкцию [0][0] = [0][0]
        lst_in[i][j], lst_in[j][i] = lst_in[j][i], lst_in[i][j]  # меняем между собой элементы относительно диагонали слева сверху - справа снизу

if lst_in == a_copy:  # если после зеркального отображения таблицы вокруг своей оси изначальный список и полученный совпадают, то матрица симметрична
    print('ДА')
else:
    print('НЕТ')
